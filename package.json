{
  "name": "atlasptrs",
  "displayName": "atlasptrs",
  "description": "Highlight comments, commands and table shortcuts for Atlas/Cartographer formatted text files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "atlas",
      "aliases": ["Atlas", "atlas"],
      "extensions": [".txt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "atlas",
      "scopeName": "text.atlas",
      "path": "./syntaxes/atlas.tmLanguage.json"
    }]
  }
}
